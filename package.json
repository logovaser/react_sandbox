{
  "name": "react_sandbox",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "uuid": "latest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-preset-react-app": "^3.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.2"
  },
  "jest": {
    "projects": [
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom",
        "testMatch": [
          "<rootDir>/**/*.spec.js"
        ],
        "transform": {
          "^.+\\.(js|jsx)$": "babel-jest",
          "^(?!.*\\.(js|jsx|json)$)": "<rootDir>/config/jest/cssTransform.js"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  }
}
